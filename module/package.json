    "@typescript-eslint/parser": "8.37.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "jest": "30.0.4",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "quicktype": "23.2.6",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "ts-patch": "3.3.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@types/lodash": "4.17.20",
    "ajv": "8.17.1",
