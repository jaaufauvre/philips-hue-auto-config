{
  "name": "hue-auto-config",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest ./tests --coverage",
    "lint": "eslint .",
    "format": "prettier --write \"../**/*.+(js|mjs|ts|json)\" --config .prettierrc.json",
    "start": "ts-node src/index.ts",
