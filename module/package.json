{
  "name": "hue-auto-config",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest ./tests --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.+(js|ts|json|eslintrc)\"",
    "start": "ts-node src/index.ts",
