    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "nodemon": "3.0.2",
    "prettier": "3.2.1",
    "quicktype": "23.0.81",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "ts-patch": "3.1.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@types/lodash": "4.14.202",
    "ajv": "8.12.0",
    "ajv-keywords": "5.1.0",
